<!--
Installation model

    1. by path & name
    path - absolute path to the file
    name - file name (relative to src)

    2. type
    * systemd-tmpfiles  - install $(project.name) to /usr/lib/tmpfiles.d/
     -->

<install>
    <item type = "systemd-tmpfiles" />
    <item path = "/etc/fty/" name = "../ftyshmcli.cfg" package = "lib" />
</install>
